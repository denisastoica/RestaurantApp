<Page x:Class="Restaurant.Views.ClientHomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="Acasă Client">

    <Grid>
        <TabControl>
            <TabItem Header="Meniu">
                <Frame x:Name="MenuFrame" NavigationUIVisibility="Hidden"/>
            </TabItem>
            <TabItem Header="Căutare">
                <Frame x:Name="SearchFrame" NavigationUIVisibility="Hidden"/>
            </TabItem>
            <TabItem Header="Coș">
                <Frame x:Name="CartFrame" NavigationUIVisibility="Hidden"/>
            </TabItem>
            <TabItem Header="Comenzi">
                <Frame x:Name="OrdersFrame" NavigationUIVisibility="Hidden"/>
            </TabItem>
        </TabControl>
    </Grid>
</Page>
